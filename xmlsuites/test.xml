    <parameter name="browser" value="chrome" />
    <parameter name="url" value="https://www.realtor.com/" />
    <parameter name="wait" value="10" />
    <test name="Example">
        <classes>
            <class name="com.selenium.cases.SearchARandomApartmentAndComparePrice">
                <methods>
                    <include name="searchARandomApartmentAndComparePrice" />
                </methods>   
            </class>
        </classes>
    </test>
 </suite>   